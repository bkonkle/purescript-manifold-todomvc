{
  "name": "purescript-recompose-todomvc",
  "version": "1.0.0",
  "description": "PureScript <<< React <<< Recompose <<< Redux $ TodoMVC",
  "scripts": {
    "build": "npm run lint && gatsby build --prefix-links",
    "clean": "if [ -d public ]; then rm -r public; fi",
    "deploy": "npm run clean && npm run build && npm run upload",
    "lint": "eslint --ext .js,.jsx src",
    "prepublish": "npm run build",
    "start": "npm run lint && gatsby develop",
    "upload": "surge public ecliptic.github.io",
    "postinstall": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bkonkle/purescript-manifold-todomvc.git"
  },
  "author": "Brandon Konkle <brandon@ecliptic.io>",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
